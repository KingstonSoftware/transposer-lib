{
  "name": "transpose-lib",
  "version": "1.0.0",
  "description": "Music transposition library",
  "main": "build/index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "npm run clean && babel src -d dist -s --ignore **/*.test.js",
    "test": "jest",
    "test:debug": "babel-node --inspect-brk ./node_modules/jest-cli/bin/jest.js --runInBand",
    "deploy": "npm publish --access public",
    "show-coverage": "open coverage/lcov-report/index.html",
    "lines-of-code": "wc -l $(git ls-files | grep '.*\\.js$')"
  },
  "jest": {
    "collectCoverage": true,
    "automock": false,
    "testPathIgnorePatterns": [
      "dist/",
      "node_modules/",
      "scratch/"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "jest-cli": "^25.1.0"
  },
  "dependencies": {}
}
